<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sb_list_ns">
    
    <cache eviction="FIFO" flushInterval="60000" size="1024" readOnly="true"/> 
    
    <resultMap id="BoardResult" type="SB_Board" ><!-- BoardResult -->
    	<result property="NUM" />
    	<result property="ISNOTICE" />
    	<result property="ISBLOCK" />
    	<result property="TITLE" />
    	<result property="WRITER" />
    	<result property="PWD" />
    	<result property="CONTENTS" />
    	<result property="IMGNAME" />
    	<result property="COUNT" />
    	<result property="REGTIME" />
    	<result property="BREF" />
    	<result property="BSTEP" />
    	<result property="BLEVEL" />
    </resultMap>
    
    
    <select id="selectList" resultMap="BoardResult">
    	SELECT * 
    	FROM SB_BOARD
		ORDER BY ISNOTICE DESC, NUM DESC
    </select>
    
    
    
    
    
    
    
</mapper>